<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="meeconf" extends="default">
		<action name="getmeeconf" method="getmeeconf" class="meeconfAction">
            <result name="login">/login.jsp</result>
            <result name="success" type="freemarker">/pages/adminmeeerence.vtl</result>
        </action>
        
        <action name="addMeeting" method="addMeeting" class="meeconfAction">
        	<result name="login">/login.jsp</result>
        	<result name="success" type="redirect">meeconf/getmeeconf</result>
        </action>
        
        <action name="delMeeting" method="delMeeting" class="meeconfAction">
        	<result name="login">/login.jsp</result>
        	<result name="success" type="redirect">meeconf/getmeeconf</result>
        </action>
        
        <action name="modMeeting" method="modMeeting" class="meeconfAction">
        	<result name="login">/login.jsp</result>
        	<result name="success" type="redirect">meeconf/getmeeconf</result>
        </action>
    </package>
</struts>
